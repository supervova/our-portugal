# ------------------------------------------------------------------------------
# WP
# ------------------------------------------------------------------------------

web/*.php
web/wp-admin
web/wp-includes
web/wp-content/*
!web/wp-content/plugins/
!web/wp-content/themes/

# Ignore everything in the "plugins" directory, except the plugins we
# specify (see the commented-out examples for hints on how to do this.)
web/wp-content/plugins/*
# !wp-content/plugins/my-single-file-plugin.php
# !wp-content/plugins/my-directory-plugin/

# Ignore everything in the "themes" directory, except the themes we specify
web/wp-content/themes/*
!web/wp-content/themes/our-portugal-2020/

